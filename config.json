{
	"name": "CA Facts Project EM",

	"namespace": "Stanford\\ProjCaFacts",

	"description": "An em to help manage CA FACTS project and interaction with GAUSS",

	"documentation": "README.md",

	"authors": [
		{
			"name": "Andrew Martin",
			"email": "andy123@stanford.edu",
			"institution": "Stanford University"
		},
        {
			"name": "Irvin Szeto",
			"email": "irvins@stanford.edu",
			"institution": "Stanford University"
		}

	],

	"permissions": [
	],

	"framework-version": 4,

	"enable-every-page-hooks-on-system-pages": false,

	"links": {
		"project": [
			{
	        "name": "CA-FACTS Invitation API Instructions",
	        "icon": "gear",
	        "url": "pages/instructions.php"
	      }
		],
		"control-center": [
	      {
	        "name": "CA-FACTS Invitation Endpoint Config",
	        "icon": "gear",
	        "url": "pages/app_config.php"
	      }
	    ]
	},

	"no-auth-pages": [
        "pages/signup-ivr",
		"pages/signup",
		"pages/kitsubmission",
		"getAsset"
	],

	"project-settings": [
        {
			"key": "em-mode",
			"name": "<b>EM Mode</b><br>This EM is used in more than one project with different functionality.  Please indicate which project this is?",
			"required": true,
			"type": "radio",
            "choices":[
                {
                  "name" : "ACCESS CODE DB",
                  "value" : "access_code_db"
                },
                {
                  "name" : "KIT ORDER (MAIN)",
                  "value" : "kit_order"
                },
                {
                  "name" : "KIT SUBMISSION",
                  "value" : "kit_submission"
                }
            ]
        },
		{
			"key": "enable-project-debug-logging",
			"name": "<b>Enable Debug Logging</b><br><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],


	"system-settings": [
		{
			"key": "enable-system-debug-logging",
			"name": "<b>Enable Debug Logging (system-wide)</b><i>(optional)</i> Requires installation and configuration of emLogger",
			"required": false,
			"type": "checkbox"
		}
	],

	"compatibility": {
		"php-version-min": "",
		"php-version-max": "",
		"redcap-version-min": "",
		"redcap-version-max": ""
	}
}
